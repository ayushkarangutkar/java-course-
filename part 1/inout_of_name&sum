import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        String name = sc.next();
        int sum = a+b;
        System.out.print("the sum of a and b ,name :- ");
        System.out.print(sum);
        System.out.print(name);
    }
}
