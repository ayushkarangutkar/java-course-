import java.util.*;
class Main
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int size,k;
        System.out.println("Enter the size :-");
        size =sc.nextInt();
        int arr[] =new int[size];
        System.out.println("Enter the element :- ");
        for(k=0;k<size;k++)
        {
            arr[k] = sc.nextInt();
        }
        System.out.println("The larger number:- ");
        int large = arr[0];
        for(int i = 0; i<size;i++)
        {
            if(large<arr[i])
            {
                large=arr[i];
            }
        }
        System.out.println("The larger the number is :- "+large);
        
    }
}
