import java.util.Vector;

public class Main
{
    public static void main(String[] args) 
    {
      
        // Create a new vector
        Vector<Integer> v = new Vector<>(3, 2);

        // Add elements to the vector
        v.addElement(1);
        v.addElement(2);
        v.addElement(3);

            System.out.println(v);
    }
}
