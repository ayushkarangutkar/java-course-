import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int arr[]= new int[20];
		int i,n,sum=0,mul=0;
		
		System.out.print("\n Enter the size of an array :- ");
		  n = sc.nextInt();
		 for(i =0; i<n; i++)
		 {
		     arr[i]=sc.nextInt();
		 }
		 
		 for(i =1; i<n; i++)
		 {
		     sum=sum+arr[i];
		 }
		 System.out.print("\n the sum of array"+" "+sum);
	}
}
