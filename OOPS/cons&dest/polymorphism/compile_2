import java.util.*;
class student
{
    
    public String displayinfo(String str1,String str2)
    {
        str1+=str2;
        System.out.println(str1);
        return;
    }
    public double displayinfo(double vr1,double vr2)
    {
        vr1+=vr2;
        System.out.println(vr1);
        return;
    }
    public int displayinfo(int var1,int var2)
    {
       var1+=var2;
       System.out.println(var1);
       return;
    }
}
public class Main
{
	public static void main(String[] args)
	{
		student s1 = new student();
		
		s1.displayinfo("method","overloading");
		s1.displayinfo(10.5,8.9);
		s1.displayinfo(10,5);
		return ;
	}
}
