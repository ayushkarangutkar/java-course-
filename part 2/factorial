import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int factorial=1;
		for(int i=n;i>0;i--)
		{
		    factorial=factorial*i;
		}
		System.out.print(" The factorial of  ");
		System.out.print(n);
		System.out.print("! is ");
		System.out.print(factorial);
	}
}
