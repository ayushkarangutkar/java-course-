import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int n=3,m=6;
	    
		for(int i=1; i<=n; i++ ){
		    for(int j=1;j<=m;j++){
		        
		        if(i==1 || j==3 || i==3 || j==4 ) {
		            System.out.print("*");
		        }
		        else {
		            System.out.print(" ");
		        }
		    }
		    System.out.print("\n");
		}
	}
}
