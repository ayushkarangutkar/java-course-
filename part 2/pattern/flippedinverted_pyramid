import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int n,i,j;
         n=5;
    
    for( i=1;i<=5;i++)
    {
        for( j=1;j<=5;j++)
        {
            if(j>=i)
            {
                System.out.print("*");
            }
            else
            {
                System.out.print(" ");
            }
        }
        System.out.println();
    }
  }
}
