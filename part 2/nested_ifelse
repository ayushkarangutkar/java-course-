import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int age = sc.nextInt();
		
		if(age>10 && age<50 )
		{
		    if(age>10 && age<18)
		    {
		        System.out.print("you are not adult");
		    }
		    else //here AND opertor check both condition and then you pass
		    {
		        System.out.print("you are adult");
		    }
		}
		else
		{
		    System.out.print("you are old");
		}
		
	}
}
