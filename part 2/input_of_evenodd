import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int x=sc.nextInt();
		
		if(x%2==0)
		{
		    System.out.print(x);
		    System.out.print(" this is an even ");
		}
		else
		{
		    System.out.print(x);
		    System.out.print(" this is a odd ");
		}
	}
}
