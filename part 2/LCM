import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		
		System.out.print(" Enter two number :- ");
		int a = sc.nextInt();
		int b = sc.nextInt();
		int max;
		
		max = (a>b)?a:b;
		do
		{
		    if(max % a==0 && max % b==0)
		    {
		        System.out.print(" LCM = ");
		        System.out.print(max);
		        break;
		    }
		    else 
		      ++max;
		}while(true);
	}
}
