import java.util.*;
public class Main
{
    public static void printname(int a)
    {
        System.out.print("The number :- ");
        System.out.println(a);
        return ;
    }
    public static void main(String[] args) 
	{
		Scanner sc= new Scanner(System.in);
		System.out.print("Enter the number :- ");
		int a=sc.nextInt();
		printname(a);
	}
}
