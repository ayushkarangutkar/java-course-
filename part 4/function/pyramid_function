import java.util.*;
import java.io.*;
public class Main
{
	public static void pyramid(int k)  
    { 
        int n=k;
		
		for(int i=1;i<=n;i++)
		{
		    for(int j=1;j<=i;j++)
		    {
		        System.out.print(j+" ");
		    }
		    System.out.println(" ");
		} 
    }
    
       public static void main(String[] args) 
	   {
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter  value:- ");
		int k=sc.nextInt();
		pyramid(k);
	   }
}
