import java.util.*;
public class Main
{
    public static void table(int n)
    {
        int a=n;
        System.out.println("The multiplication of "+a);
        for(int i=1;i<11;i++) 
        {
            System.out.println(n*i);
        }
    }
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter the number :- ");
		int n = sc.nextInt();
		
		table(n);
	}
}
