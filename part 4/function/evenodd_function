import java.util.*;
public class Main
{
    public static void even_odd(int n)
    {
        if(n%2==0)
        {
            System.out.println(n+" "+"this is even number");
            return ;
        }
        else{
            System.out.print(n+" "+"this is odd number");
            return;
        }
        
    }
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter the number :- ");
		int n = sc.nextInt();
		
		even_odd(n);
	}
}
