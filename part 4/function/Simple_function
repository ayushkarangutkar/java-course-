import java.util.*;
public class Main
{
    public static void printname(String name)
    {
        System.out.print("The name :- ");
        System.out.println(name);
        return ;
    }
    public static void main(String[] args) 
	{
		Scanner sc= new Scanner(System.in);
		System.out.print("Enter the name :- ");
		String name=sc.next();
		printname(name);
	}
}
